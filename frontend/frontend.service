[Unit]
Description=APLaC-Chat Frontend  Web App
After=network.target

[Service]
ExecStart=/home/apps/prg/dotnet/sdk/dotnet frontend.dll
WorkingDirectory=/home/apps/prg/aplac/frontend/src/bin/
User=apps
Restart=on-failure
EnvironmentFile=/etc/sysconfig/frontend.env

[Install]
WantedBy=multi-user.target
